{
    "author": "Matheus Castiglioni <matheushcastiglioni@gmail.com>",
    "bugs": {
        "url": "https://gitlab.com/mahenrique94/my-instagram-api/issues"
    },
    "dependencies": {
        "@koa/cors": "^3.0.0",
        "boom": "^7.3.0",
        "bull": "^3.14.0",
        "bull-board": "^0.7.0",
        "crypto": "^1.0.1",
        "date-fns": "^2.14.0",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "fastest-validator": "^1.2.0",
        "file-type": "^14.5.0",
        "generate-sms-verification-code": "^1.0.5",
        "i18next": "^19.4.4",
        "jsonwebtoken": "^8.5.1",
        "koa": "^2.11.0",
        "koa-body": "^4.1.3",
        "koa-router": "^8.0.8",
        "mysql2": "^2.1.0",
        "nodemailer": "^6.4.6",
        "pug": "^2.0.4",
        "sequelize": "^5.21.6",
        "sequelize-cli": "^5.5.1",
        "uuid": "^7.0.3"
    },
    "description": "A minimalist Instagram API version",
    "devDependencies": {
        "@commitlint/cli": "^8.3.5",
        "@commitlint/config-conventional": "^8.3.4",
        "eslint": "^6.8.0",
        "eslint-plugin-import-helpers": "^1.0.2",
        "eslint-plugin-jest": "^23.8.2",
        "eslint-plugin-prettier": "^3.1.3",
        "husky": "^4.2.5",
        "jest": "^25.4.0",
        "nodemon": "^2.0.3",
        "prettier": "^2.0.5",
        "supertest": "^4.0.2"
    },
    "homepage": "https://gitlab.com/mahenrique94/my-instagram-api#readme",
    "keywords": [
        "Node",
        "NodeJS"
    ],
    "license": "ISC",
    "main": "./src/index.js",
    "name": "my-instagram-api",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@gitlab.com/mahenrique94/my-instagram-api.git"
    },
    "scripts": {
        "ci": "npm run lint && npm t",
        "code:check": "prettier --config ./.prettierrc --check './src/**/*.js'",
        "code:format": "prettier --config ./.prettierrc --write './src/**/*.js'",
        "commitlint": "commitlint -E HUSKY_GIT_PARAMS",
        "db": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
        "db:create": "sequelize db:create",
        "db:drop": "sequelize db:drop",
        "db:migrate": "sequelize db:migrate",
        "db:seed": "sequelize db:seed",
        "db:unseed": "sequelize db:seed:undo:all",
        "db:unseed:last": "sequelize db:seed:undo",
        "db:unmigrate": "sequelize db:migrate:undo:all",
        "db:unmigrate:last": "sequelize db:migrate:undo",
        "dev": "nodemon",
        "jslint": "eslint .",
        "jslint:fix": "eslint . --fix",
        "lint": "npm run jslint",
        "lint:fix": "npm run jslint:fix",
        "pre-commit": "npm run code:format && npm run lint",
        "queues": "nodemon ./src/queues --watch './src/**/*(config|queues|templates)/**'",
        "stack:dev": "docker-compose up -d",
        "stack:dev:build": "docker-compose build",
        "stack:dev:clean": "docker-compose rm -s -f",
        "stack:dev:start": "docker-compose start",
        "stack:dev:stop": "docker-compose stop",
        "test": "jest --config './jest.config.js' --maxWorkers=2 --passWithNoTests",
        "test:e2e": "jest --config './jest.e2e.config.js' --maxWorkers=2 --passWithNoTests",
        "test:e2e:w": "npm run test:e2e -- --watchAll",
        "test:w": "npm t -- --watchAll"
    },
    "version": "1.0.0"
}
